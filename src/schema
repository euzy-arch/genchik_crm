interface Transaction {
  id: number;
  date: string;
  description: string;
  category: string;
  amount: number;
  type: 'income' | 'expense';
  category_id?: number;
}

interface Category {
  id: number;
  name: string;
}

interface AIAnalysis {
  id: number;
  analysis_type: string;
  period_start: string;
  period_end: string;
  content: any;
  summary: string;
  insights: any;
  created_at: string;
}

interface Message {
  id: number;
  text: string;
  sender: 'user' | 'ai';
  timestamp: Date;
}